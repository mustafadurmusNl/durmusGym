{
  "name": "durmusgym",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "clean:cypress": "rimraf \"cypress/screenshots/\" \"cypress/videos/\" \"cypress/reports/\"",
    "clean:docs": "rimraf \"docs/\"",
    "clean:all": "npm run clean:cypress && npm run clean:docs",
    "cypress:open": "cypress open",
    "e2e-test": "cypress run",
    "test:report": "node generate-report.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=./jest.config.js",
    "test:coverage": "cross-env BABEL_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --coverageReporters=html",
    "start": "node server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "build": "npm run build",
    "heroku-postbuild": "npm run build --prefix client && npm run build --prefix server"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.1.2",
    "concurrently": "^7.6.0",
    "cpx": "^1.5.0",
    "cross-env": "^10.0.0",
    "cypress": "^15.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "i18next": "^21.10.0",
    "lucide-react": "^0.511.0",
    "nodemailer": "^7.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^11.18.6",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.1",
    "tsx": "^4.20.5",
    "validator": "^13.15.0"
  },
  "engines": {
    "node": "22.x",
    "npm": "11.x"
  }
}
